<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView24"
        android:layout_width="398dp"
        android:layout_height="387dp"
        android:layout_marginTop="24dp"
        android:background="#2ECCFA"
        android:text="배송정보"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/tv_recive"
        android:layout_width="243dp"
        android:layout_height="50dp"
        android:layout_marginTop="32dp"
        android:background="#FFFFFF"
        android:gravity="center_horizontal"
        android:textSize="30sp"
        android:textAlignment="center"
        android:textColor="#000000"
        android:hint="수령인 이름"
        app:layout_constraintStart_toEndOf="@+id/imageView19"
        app:layout_constraintTop_toTopOf="@+id/textView24" />

    <TextView
        android:id="@+id/tv_things"
        android:layout_width="243dp"
        android:layout_height="50dp"
        android:layout_marginTop="8dp"
        android:background="#FFFFFF"
        android:textSize="30sp"
        android:textAlignment="center"
        android:textColor="#000000"
        android:gravity="center_horizontal"
        android:fontFamily="@font/nanumb"
        android:hint="물품 이름"
        app:layout_constraintStart_toEndOf="@+id/imageView17"
        app:layout_constraintTop_toBottomOf="@+id/tv_recive" />

    <TextView
        android:id="@+id/tv_togo"
        android:layout_width="243dp"
        android:layout_height="50dp"
        android:layout_marginTop="4dp"
        android:background="#FFFFFF"
        android:gravity="center_horizontal"
        android:fontFamily="@font/nanumb"
        android:textSize="30sp"
        android:textAlignment="center"
        android:textColor="#000000"
        android:hint="목적지"
        app:layout_constraintStart_toEndOf="@+id/imageView16"
        app:layout_constraintTop_toBottomOf="@+id/tv_things" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="400dp"
        android:layout_height="165dp"
        android:layout_marginTop="260dp"
        android:background="#0080FF"
        android:text="수령인 만족도"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="62dp"
        android:layout_height="50dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="56dp"
        android:src="@drawable/find"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="62dp"
        android:layout_height="50dp"
        android:layout_marginTop="8dp"
        android:src="@drawable/things_name"
        app:layout_constraintStart_toStartOf="@+id/imageView19"
        app:layout_constraintTop_toBottomOf="@+id/imageView19" />

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="62dp"
        android:layout_height="50dp"
        android:layout_marginTop="4dp"
        android:src="@drawable/end"
        app:layout_constraintStart_toStartOf="@+id/imageView17"
        app:layout_constraintTop_toBottomOf="@+id/imageView17" />

    <ImageView
        android:id="@+id/iv_delivertStat"
        android:layout_width="407dp"
        android:layout_height="161dp"
        android:layout_marginBottom="68dp"
        android:src="@drawable/baesong"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"

        app:layout_constraintTop_toBottomOf="@+id/textView24"
        app:layout_constraintVertical_bias="0.747" />

    <Button
        android:id="@+id/btn_final2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:text="배송완료처리"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_delivertStat"
        app:layout_constraintVertical_bias="0.592" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="배송 예정 시간"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.888"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView20" />

    <TextView
        android:id="@+id/tv_time"
        android:layout_width="22dp"
        android:layout_height="22dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="460dp"
        android:text="0초"
        app:layout_constraintStart_toEndOf="@+id/textView13"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:text="배송현황"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView24" />

    <TextView
        android:id="@+id/tv_re_SatisF"
        android:layout_width="253dp"
        android:layout_height="79dp"
        android:layout_marginTop="32dp"
        android:background="#FFFFFF"
        android:fontFamily="@font/nanumb"
        android:gravity="center_horizontal"
        android:hint="사용자 만족도 표시"
        android:textColor="#000000"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView20" />

    <Button
        android:id="@+id/btn_refresh"
        android:layout_width="106dp"
        android:layout_height="50dp"
        android:layout_marginTop="372dp"
        android:text="새로고침"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>